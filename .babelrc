{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 1%, last 2 versions, not ie <= 8",
        "useBuiltIns": "usage",
        "corejs": "3"
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allowNamespaces": true,
        "allExtensions": true
      }
    ]
  ],
  "plugins": [["@babel/plugin-transform-runtime", { "corejs": 3 }]]
}
